    "cypress": "^15.10.0",
    "happy-dom": "^20.7.0",
    "msw": "^2.12.10",
    "shadcn": "^3.8.5",
    "start-server-and-test": "^2.1.3",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.17"
