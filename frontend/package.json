{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:test": "vite --port 3001 --mode test",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "vitest run",
    "test:watch": "vitest",
